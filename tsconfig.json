{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "lib": ["es2018", "dom"],
        "outDir": "dist",
        "noEmitOnError": true,
        "strict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "resolveJsonModule": true,
        "baseUrl": "./",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "experimentalDecorators": true,
        "pretty": true,
        "skipLibCheck": true,
        "jsx": "react"
    }
}
